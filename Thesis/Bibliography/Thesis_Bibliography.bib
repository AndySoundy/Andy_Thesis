@ARTICLE{Johnsen:2005,
  author = {Sonke J\"ohnsen and Kenneth J. Lohmann},
  title = {The Physics and Neurobiology of Magnetoreception},
  journal = {Nature Reviews Neuroscience},
  year = {2005},
  volume = {6},
  pages = {703-712},
  notes = {IMPORTANCE: 3.5*. Goes into the three main theories for magnetoreception
	i.e. induction, ferrimagnetic crystals and optical pumping. Like
	Walker:2002 this article proposes that magnetically sensitive animals
	could obtain positional information by the inclination angle of the
	field and local vairations within the general field. Concludes there
	isn't enough evidence to decide on the merits of induction-based
	navigation but adds that it would be highly impractical for any land-based
	animal to empoly such a tool given the resistivity of air.},
  review = {IMPORTANCE: 3*. Goes into the difficulties involved with locating
	the magnetoreceptors in animals, namely, magnetic fields pass through
	normal tissue easily and so the detectors need not be on the surface,
	the detectors may be very small or dispersed throughout the body
	with no centralised detector and that there is no human analogue
	to magnetoreception which we could base our theories on. Goes into
	induction-based magnetoreception and quotes Paulin:1995. However
	uses the work of Walker:2003, Kirschvink:2001 to say that the magnet
	placed in the nasal cavity of the ray shouldn't have effected induction-based
	magnetoreception. Goes into the other two competing theories of magnetoreception
	as well, i.e. ferri-magnetic crystals and optically-pumped radical
	pairs.},
  url = {http://biology.duke.edu/johnsenlab/pdfs/pubs/nrn%20review.pdf}
}

