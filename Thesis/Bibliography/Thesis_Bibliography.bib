@ARTICLE{Paulin:1995,
  author = {Michael G. Paulin},
  title = {Electroreception and the Compass Sense of Sharks},
  journal = {Journal of Theoretical Biology},
  year = {1995},
  volume = {174},
  pages = {325-339},
  review = {IMPORTANCE: 5*+. Mike Paulin's first article on shark navigation,
	obviously this is super important and by this time you probably have
	it memorized but here we go. He goes into the then current work in
	the field by Kalmijn and Klimley. He investigates Kalmijn's ideas
	of pasive and active navigation but points towards the problems of
	sharks being unable to detect dc signals and of the issues involved
	in Kalmijn's passive navigation wherein it would seem Kalmijn proposes
	that the shark swims out of any moving water and then uses that stationary
	water as a guide as to where it should head as it swims back into
	the current. Paulin points also to the issue of naturally occurring
	electric fields in the order of hundreds of microvolts due to the
	movement of the water and the ions within and then points to the
	dc filter and says that for the time-period that the ampullae of
	the shark cover the fields in the water are considered dc and so
	not picked up by the animals. For a short time he goes into the idea
	of electrical lensing that would occur around the resistive body
	of the shark in the conducting medium of the sea and then points
	to that increasing the voltages across an ampullae in the shark rather
	than some lone ampullae and so increasing the animal's sensitivity.
	Goes extensively into the idea of wave-analysis as a way for the
	shark to determine its heading especially working out the differences
	in frequency and amplitude involved in heading north-south compared
	to east-west. By comparing the inputs from the electrosense and the
	vestibular sense the shark could determine a heading direction undettered
	by current drift. He backs up his claim of waveform analysis by pointing
	out that 19% of vestibulo-cerebellar cells in dogfish respond at
	twice the head oscillation frequency. Finally he points to the hyoid
	group of ampullae as a possible base for navigation citing migratory
	species that have hyoid ampullae only and eat plankton and carnivorour
	species that are non-migratory and have other ampullae but not hyoid
	ampullae.},
  url = {http://dx.doi.org/10.1006/jtbi.1995.0102}
}

