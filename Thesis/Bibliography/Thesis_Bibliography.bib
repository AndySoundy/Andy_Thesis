@ARTICLE{Molteno:2009,
  author = {T. C. A. Molteno and W. L. Kennedy},
  title = {Navigation by Induction-Based Magnetoreception in Elasmobranch Fishes},
  journal = {Journal of Biophysics},
  year = {2009},
  volume = {2009},
  pages = {6 pages},
  review = {IMPORTANCE: 5*+. Tim's latest paper on shark navigation. Quantitatively
	addresses the electric fields that the shark should experience while
	it swims through the earth's magnetic field. Proposes that the shark
	uses a ration of electric field amplitudes at different frequencies
	to determine it's compass heading which should be independent of
	swimming speed and the strength of the magnetic field. This ratio
	is dependent on the swimming modulation amplitude (a) but for values
	of a less than 1 the ratio is only weakly dependent on a. Both the
	electrical amplitudes involved should be detectable by the ampullae
	based on the thresholds determined in Kajiura:2002 as well as others.
	While a linear model of the neural response would be unable to account
	for drift we see from studies such as Montgomery:1999 that the neural
	response is indeed non-linear. Also addresses the work of Hodson:2000
	and others where magnets inserted in the nasal cavity of the short-tailed
	stingray would interrupt an induction-based navigation sense unless
	the fish was kept rigid to an accuracy of ~100um. Possible work on
	the theory involves further investigation of the non-linear response
	theory and more complex analysis of migration-style swimming kinematics.},
  url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2814134/}
}

