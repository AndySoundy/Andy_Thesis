@ARTICLE{Carey:1990,
  author = {F. G. Carey and J. V. Scharold},
  title = {Movements of blue sharks (\textit{Prionace glauca}) in depth and
	course},
  journal = {Marine Biology},
  year = {1990},
  volume = {106},
  pages = {329-342},
  owner = {souan326},
  review = {IMPORTANCE: 4*. A study performed over a decade with blue sharks in
	the North Atlantic off the coast of the US. The sharks often held
	steady directions for long distances over day and night with only
	minor variations in direction. Two sharks stayed below 50m for almost
	all night while maintaining smooth headings which would suggest that
	they are navigating using cues other than purely celestial ones as
	at those depths starlight is diffuse and difficult to resolve. During
	the day the sharks made periodic dives to depths ~200-400m with periods
	between 1 and 3.5hrs. It was proposed that this could be a hunting
	technique, either to locate scents that had been horizontally distributed
	by current shear between layers of differing ocean density. It was
	also proposed that as the sharks commonly dove more quickly than
	they surfaced that they were visually searching for prey by attempting
	to locate them from beneath by backlighting them against the light
	from the surface, this was reinforced by the fact that almost all
	deep dives were only undertaken during daylight.},
  timestamp = {2013.03.30},
  url = {http://link.springer.com/article/10.1007/BF01344309}
}

