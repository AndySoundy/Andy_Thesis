@ARTICLE{Klimley:1993,
  author = {A. P. Klimley},
  title = {Highly directional swimming by scalloped hammerhead sharks, Sphyrna
	lewini, and subsurface irradiance, temperature, bathymetry, and geomagnetic
	field},
  journal = {Marine Biology},
  year = {1993},
  volume = {117},
  pages = {1-22},
  note = {IMPORTANCE: 5*. An excellent paper, the author tagged four scalloped
	hammerheads off Las Animas Island in the Sea of Cortez. Highly directional
	swimming was recorded when the sharks ventured out at night to hunt.
	The author correlated their movements to possible variations in temperature,
	current, ground topography (which the animals were unable to see
	from the depths they were swimming at), light levels (to determine
	whether the animals could have been navigating by star/moonlight)
	and variations in the earths magnetic field. No correlations were
	found for any of the values that could explain the sharks swimming
	very similar geographic routes except for using variations in the
	earths magnetic field. Less than a 50nT rise or fall between successive
	maxima or minima in the magnetic field but it appears that the sharks
	swam along the boundary between magnetic slope and plateau as well
	as in valleys or along ridges. In several instances the sharks encountered
	a "wall" of steep geomagnetic increases whereupon the animals reversed
	direction and swam back the way they had come. A graph is provided
	in the article (Fig. 11) that shows that the sharks was actively
	swimming at a depth that gave it the greatest geomagnetic intensity
	gradient, almost three times the gradient at the surface. In conclusion
	proposed that perhaps the shark compared readings from ampullae across
	the body to determine the magnetic field gradient.},
  url = {http://link.springer.com/article/10.1007%2FBF00346421?LI=true}
}

