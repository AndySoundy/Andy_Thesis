@ARTICLE{,
  author = {Ad. J. Kalmijn,},
  title = {Electric and Magnetic Field Detection in Elasmobranch Fishes},
  journal = {Science},
  year = {1982},
  volume = {218},
  pages = {916-918},
  number = {4575},
  month = {November},
  note = {IMPORTANCE: 3.5*. An early work on the electric and magnetic sensitivites
	of elasmobranches. Found that blue sharks and dogfish would preferentially
	attack an artifical dipole that was hidden, even when placed next
	to a piece of food which suggests that for short-range feeding behaviour
	the electric sense is more important than even smell. Measured the
	response rate as 5nV/cm but this was a conservative estimate as it
	assumed that the shark was alligned directly with the dipole moment
	created by the electrodes of the experiment. A short article that
	mostly focuses on the responses to electric fields it does mention
	magnetic field responses however it is only briefly and concludes
	simply that the rays used in the experiment could detect the magnetic
	field but remains unsure whether the rays were directly detecting
	the magnetic field or the induced voltage by swimming near the magnetic
	field.},
  owner = {souan326},
  timestamp = {2013.03.08},
  url = {http://www.jstor.org/sici?sici=0036-8075%2819821126%293%3A218%3A4575%3C916%3AEAMFDI%3E2.0.CO%3B2-J}
}

