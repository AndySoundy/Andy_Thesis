@INBOOK{,
  chapter = {4},
  pages = {125-144},
  title = {Handbook of Sensory Physiology},
  publisher = {Springer Verlag},
  year = {1974},
  editor = {A. Fernard},
  author = {Richard W. Murray},
  volume = {3},
  note = {IMPORTANCE: 5*. A good summary on the structure and response of the
	ampullae of Lorenzini. Apparently there are ~ as many nerves dedicated
	to the ampullae of Lorenzini as there are to the eyes or ears which
	suggests a highly specialised sense. Contains good schematic details
	(typical sizes, resistivity etc) of the canals themselves. There
	is evidence that ampullae can detect small changes in salinity of
	seawater (32-33%), possibly due to the effect of raising/lowering
	the electrical properties at the pore interface. Also cites evidence
	that a massive field ~ 100uVm-1 and higher produce less response
	than lower fields and even produce reversed responses. Longer canals
	are in general more sensitive than shorter ones but due to the capacity
	of the canals there is a loss of higher frequencies in longer canals.
	Hyoid canals are the longest group of ampullae.},
  owner = {souan326},
  timestamp = {2013.03.04}
}

@ARTICLE{Paulin:1995,
  author = {Michael G. Paulin},
  title = {Electroreception and the Compass Sense of Sharks},
  journal = {Journal of Theoretical Biology},
  year = {1995},
  volume = {174},
  pages = {325-339},
  note = {IMPORTANCE: 5*+. Mike Paulin's first article on shark navigation,
	obviously this is super important and by this time you probably have
	it memorized but here we go. He goes into the then current work in
	the field by Kalmijn and Klimley. He investigates Kalmijn's ideas
	of pasive and active navigation but points towards the problems of
	sharks being unable to detect dc signals and of the issues involved
	in Kalmijn's passive navigation wherein it would seem Kalmijn proposes
	that the shark swims out of any moving water and then uses that stationary
	water as a guide as to where it should head as it swims back into
	the current. Paulin points also to the issue of naturally occurring
	electric fields in the order of hundreds of microvolts due to the
	movement of the water and the ions within and then points to the
	dc filter and says that for the time-period that the ampullae of
	the shark cover the fields in the water are considered dc and so
	not picked up by the animals. For a short time he goes into the idea
	of electrical lensing that would occur around the resistive body
	of the shark in the conducting medium of the sea and then points
	to that increasing the voltages across an ampullae in the shark rather
	than some lone ampullae and so increasing the animal's sensitivity.
	Goes extensively into the idea of wave-analysis as a way for the
	shark to determine its heading especially working out the differences
	in frequency and amplitude involved in heading north-south compared
	to east-west. By comparing the inputs from the electrosense and the
	vestibular sense the shark could determine a heading direction undettered
	by current drift. He backs up his claim of waveform analysis by pointing
	out that 19% of vestibulo-cerebellar cells in dogfish respond at
	twice the head oscillation frequency. Finally he points to the hyoid
	group of ampullae as a possible base for navigation citing migratory
	species that have hyoid ampullae only and eat plankton and carnivorour
	species that are non-migratory and have other ampullae but not hyoid
	ampullae.},
  url = {http://www.sciencedirect.com/science/article/pii/S0022519385701026#}
}

@ARTICLE{Videler:1984,
  author = {J. J. Videler and F. Hess},
  title = {Fast Continuous Swimming of  two Pelagic Predators Saithe (Pollachius
	Virens) and Mackrel (Scomber Scombrus): A Kinematic Analysis},
  journal = {Journal of Experimental Biology},
  year = {1984},
  volume = {109},
  pages = {209-228},
  notes = {IMPORTANCE: 3*. While neither fish analysed were elasmobranches and
	the swimming styles concerned were not slow migratory style but fast
	continuous swimming the article contains several potentially useful
	elements and also goes into an efficiency analysis though whether
	this will be relevant to the thesis remains to be seen. The analysis
	only concerned itself with straight, fast, continuous swimming styles
	and made the useful move to use dimensionless units using the fishes
	length as a unit of measurement and the time period of one tail movement
	to be the unit of time. During steady swimming it was found that
	the pectoral and pelvic fins pressed against the body and the anterior
	parts of the dorsal and anal fins collapsed. Found a general equation,
	as one might expect, was of the form f(t) = a0 + b0t - a1*cos(2*pi*t/T)
	+ b1*sin(2*pi*t/T).},
  url = {http://jeb.biologists.org/content/109/1/209.full.pdf}
}

