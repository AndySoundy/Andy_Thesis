@ARTICLE{,
  author = {Marcelo Camperi, Timothy C. Tricas and Brandon E. Brown},
  title = {From Morphology to Neural Information: The Electric Sense of the
	Skate},
  journal = {PLoS Computational Biology},
  year = {2007},
  volume = {3},
  month = {June},
  note = {IMPORTANCE: 4.5*. An excellent modelling paper on the electric sense
	of skates. While the paper concerned itself with the use of the electric
	sense for prey detection and not navigation it is highly likely that
	many of the methods and implementation could be borrowed to create
	a realistic model of the electric sense in sharks and then use that
	to test the response of neurons to the shark moving in the earth's
	magnetic field. They took good account of the electrical properties
