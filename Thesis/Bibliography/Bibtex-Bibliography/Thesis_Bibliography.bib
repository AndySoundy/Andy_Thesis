@ARTICLE{,
  author = {P. W. Webb and S. Keys},
  title = {Swimming Kinematics of Sharks},
  journal = {Fishery Bulletin},
  year = {1982},
  volume = {80},
  pages = {803-812},
  number = {4},
  note = {IMPORTANCE: 3*. A kinematic analysis across six species (nurse, bull,
	lemon, leopard, bonnethead, blacktip sharks) of which the nurse,
	bull and lemon were in the range 2-2.5m in length. Found that for
	the blacktip (the only shark that could be induced to swim across
	a large enough range of speeds) that tail-beat frequency increased
	linearly with speed. Introduces multiple classification types for
	swimming styles across sharks and teleost fishes ranging from the
	more fusiform sharks to the more carangiform. Defines anguilliform
	swimming mode as the body containing more than one half wavelength
	within the body (often one or more complete waves). Then carangiform
	swimming is where the body contains less than one half wavelength
	and lateral displacements increase rapidly over the posterior third
	or half of the body. Then sub-carangiform swimming was defined to
	be fish with wave patterns of the anguilliform mode and amplitude
	changes similar to the carangiform mode. The sharks in the study
	were all found to be sub-carangiform swimmers.},
  owner = {souan326},
  timestamp = {2013.03.07},
  url = {http://fishbull.noaa.gov/80-4/webb.pdf}
}

